{
  "parser": "babel-eslint",
  "env": {
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "useTabs": true,
        "trailingComma": "es5"
      }
    ],
    "no-console": "warn",
    "no-undef": "warn",
    "no-unused-vars": "warn"
  }
}